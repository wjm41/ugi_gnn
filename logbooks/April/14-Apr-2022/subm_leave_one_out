#!/bin/bash
#SBATCH -J subm_leave_one_out
#SBATCH -A LEE-WJM41-SL2-CPU
#SBATCH --nodes=1
#SBATCH --ntasks=1
 #SBATCH --cpus-per-task=1
#SBATCH --time=3:00:00
